{
    "plugins": [
      "plugins/markdown"
    ],
    "source": {
      "include": ["src"],
      "excludePattern": ".+\\.(test|stubs).js(x|doc)?$"
    },
    "opts": {
      "destination": "docs",
      "readme": "README.md",
      "recurse": true,
      "template": "node_modules/@pixi/jsdoc-template"
    }
}
